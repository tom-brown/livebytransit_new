<div class="page-header">
  <h1>
    Listings
    <a href="/listings/new" class="btn btn-lg btn-success">New Listing</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Address</th>
        <th>Lat</th>
        <th>Lng</th>
        <th>User</th>
        <th>Beds</th>
        <th>Baths</th>
        <th>Half baths</th>
        <th>Description</th>
        <th>Garage spaces</th>
        <th>Assigned parking</th>
        <th>Active</th>
        <th>Age</th>
        <th>Size</th>
        <th>City</th>
        <th>Heat gas</th>
        <th>Heat radiator</th>
        <th>Ac window units</th>
        <th>Ac central</th>
        <th>List date</th>
        <th>Pin</th>
        <th>Recent rehab</th>
        <th>Vintage</th>
        <th>New construction</th>
        <th>Price</th>
        <th>Rental</th>
        <th>State</th>
        <th>St num</th>
        <th>Street</th>
        <th>St suffix</th>
        <th>Cp</th>
        <th>Unit</th>
        <th>Zip code</th>
        <th>Property type</th>
        <th>Laundry</th>
        <th>Cats</th>
        <th>Dogs</th>
        <th>Available date</th>
        <th>Image urls</th>
        <th>Neighborhood</th>
        <th>Picture count</th>
        <th>Mls</th>
        <th>Lo mlsid</th>
        <th>La first name</th>
        <th>La last name</th>
        <th>Total baths</th>
        <th>Actions</th>
      </tr>

      <% @listings.each do |listing| %>
      <tr>
        <td><%= listing.address %></td>
        <td><%= listing.lat %></td>
        <td><%= listing.lng %></td>
        <td><%= listing.user.email %></td>
        <td><%= listing.beds %></td>
        <td><%= listing.baths %></td>
        <td><%= listing.half_baths %></td>
        <td><%= listing.description %></td>
        <td><%= listing.garage_spaces %></td>
        <td><%= listing.assigned_parking %></td>
        <td><%= listing.active %></td>
        <td><%= listing.age %></td>
        <td><%= listing.size %></td>
        <td><%= listing.city.name %></td>
        <td><%= listing.heat_gas %></td>
        <td><%= listing.heat_radiator %></td>
        <td><%= listing.ac_window_units %></td>
        <td><%= listing.ac_central %></td>
        <td><%= listing.list_date %></td>
        <td><%= listing.pin %></td>
        <td><%= listing.recent_rehab %></td>
        <td><%= listing.vintage %></td>
        <td><%= listing.new_construction %></td>
        <td><%= listing.price %></td>
        <td><%= listing.rental %></td>
        <td><%= listing.state %></td>
        <td><%= listing.st_num %></td>
        <td><%= listing.street %></td>
        <td><%= listing.st_suffix %></td>
        <td><%= listing.cp %></td>
        <td><%= listing.unit %></td>
        <td><%= listing.zip_code %></td>
        <td><%= listing.property_type %></td>
        <td><%= listing.laundry %></td>
        <td><%= listing.cats %></td>
        <td><%= listing.dogs %></td>
        <td><%= listing.available_date %></td>
        <td><%= listing.image_urls %></td>
        <td><%= listing.neighborhood.name %></td>
        <td><%= listing.picture_count %></td>
        <td><%= listing.mls_id %></td>
        <td><%= listing.lo_mlsid %></td>
        <td><%= listing.la_first_name %></td>
        <td><%= listing.la_last_name %></td>
        <td><%= listing.total_baths %></td>
        <td>
          <a href="/listings/<%= listing.id %>" class="btn btn-primary">Show</a>
          <a href="/listings/<%= listing.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_listing/<%= listing.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
