<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Address</th>
        <th>Lat</th>
        <th>Lng</th>
        <th>User</th>
        <th>Beds</th>
        <th>Baths</th>
        <th>Half baths</th>
        <th>Description</th>
        <th>Garage spaces</th>
        <th>Assigned parking</th>
        <th>Active</th>
        <th>Age</th>
        <th>Size</th>
        <th>City</th>
        <th>Heat gas</th>
        <th>Heat radiator</th>
        <th>Ac window units</th>
        <th>Ac central</th>
        <th>List date</th>
        <th>Pin</th>
        <th>Recent rehab</th>
        <th>Vintage</th>
        <th>New construction</th>
        <th>Price</th>
        <th>Rental</th>
        <th>State</th>
        <th>St num</th>
        <th>Street</th>
        <th>St suffix</th>
        <th>Cp</th>
        <th>Unit</th>
        <th>Zip code</th>
        <th>Property type</th>
        <th>Laundry</th>
        <th>Cats</th>
        <th>Dogs</th>
        <th>Available date</th>
        <th>Image urls</th>
        <th>Neighborhood</th>
        <th>Picture count</th>
        <th>Mls</th>
        <th>Lo mlsid</th>
        <th>La first name</th>
        <th>La last name</th>
        <th>Total baths</th>
        <th>Actions</th>
      </tr>

      <% @listings.each do |listing| %>
      <tr>
        <td><%= listing.address %></td>
        <td><%= listing.lat %></td>
        <td><%= listing.lng %></td>
        <td>
          <% if listing.user.present? %>
            <a href="/users/<%= listing.user.id %>">
              <%= listing.user.email %>
            </a>
          <% end %>
        </td>
        <td><%= listing.beds %></td>
        <td><%= listing.baths %></td>
        <td><%= listing.half_baths %></td>
        <td><%= listing.description %></td>
        <td><%= listing.garage_spaces %></td>
        <td><%= listing.assigned_parking %></td>
        <td><%= listing.active %></td>
        <td><%= listing.age %></td>
        <td><%= listing.size %></td>
        <td>
          <% if listing.city.present? %>
            <a href="/cities/<%= listing.city.id %>">
              <%= listing.city.name %>
            </a>
          <% end %>
        </td>
