<div class="page-header">
  <h1>
    Metra assigns
    <a href="/metra_assigns/new" class="btn btn-lg btn-success">New Metra assign</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Metra line</th>
        <th>Metra station</th>
        <th>Actions</th>
      </tr>

      <% @metra_assigns.each do |metra_assign| %>
      <tr>
        <td><%= metra_assign.metra_line.created_at %></td>
        <td><%= metra_assign.metra_station.lat %></td>
        <td>
          <a href="/metra_assigns/<%= metra_assign.id %>" class="btn btn-primary">Show</a>
          <a href="/metra_assigns/<%= metra_assign.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_metra_assign/<%= metra_assign.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
