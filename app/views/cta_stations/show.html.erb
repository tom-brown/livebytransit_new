          <a href="/cta_lines/<%= cta_line.id %>">
            <%= cta_line.name %>
          </a>
          <div class="btn-group btn-group-xs pull-right">
            <a href="/cta_lines/<%= cta_line.id %>" class="btn btn-primary">
