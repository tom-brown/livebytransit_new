<div class="page-header">
  <h1>
    Listings searches
    <a href="/listings_searches/new" class="btn btn-lg btn-success">New Listings search</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Listing</th>
        <th>Search</th>
        <th>Actions</th>
      </tr>

      <% @listings_searches.each do |listings_search| %>
      <tr>
        <td><%= listings_search.listing.address %></td>
        <td><%= listings_search.search_id %></td>
        <td>
          <a href="/listings_searches/<%= listings_search.id %>" class="btn btn-primary">Show</a>
          <a href="/listings_searches/<%= listings_search.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_listings_search/<%= listings_search.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
